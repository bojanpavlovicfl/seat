{
  "name": "eveseat/seat",
  "description": "Simple Eve Api Tool",
  "keywords": [
    "eve",
    "online",
    "corporation",
    "management",
    "api"
  ],
  "license": "GPL-2.0",
  "type": "project",
  "minimum-stability": "alpha",
  "prefer-stable": true,
  "require": {
    "php": "^7.2",
    "fideloper/proxy": "^4.0",
    "laravel/framework": "^6.2",
    "laravel/tinker": "^1.0",
    "eveseat/api": "dev-4.0.x",
    "eveseat/console": "dev-4.0.x",
    "eveseat/eveapi": "dev-4.0.x",
    "eveseat/notifications": "dev-4.0.x",
    "eveseat/services": "dev-4.0.x",
    "eveseat/web": "dev-4.0.x"
  },
  "require-dev": {
    "facade/ignition": "^1.4",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^3.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
